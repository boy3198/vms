                logMsg.indexOf('dp3.qq.com') !== -1 ||
                logMsg.indexOf('btrace.video.qq.com')
              ) {
                return false;
              }
            } else if(logLevel === '4') {
              // 无效js错误过滤
              if(
                logMsg.indexOf('Script error') === 0 ||
                logMsg.indexOf('tipsToShow is not defined') !== -1 ||
                logMsg.indexOf('“txv”未定义') !== -1 ||
                logMsg.indexOf('chrome-extension') !== -1
              ) {
                return false;
              }
            }
          }
          return msg;
        }
    })
    window.aegis = aegis;
})()
</script>
